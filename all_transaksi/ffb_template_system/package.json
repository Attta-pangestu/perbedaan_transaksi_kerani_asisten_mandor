{
  "name": "ffb-template-system-frontend",
  "version": "1.0.0",
  "description": "Frontend assets for FFB Template System",
  "main": "src/web/static/js/app.js",
  "scripts": {
    "build": "npm run build-css && npm run build-js",
    "build-css": "sass src/web/static/scss:src/web/static/css --style compressed",
    "build-js": "webpack --mode production",
    "dev": "concurrently \"npm run watch-css\" \"npm run watch-js\"",
    "watch-css": "sass src/web/static/scss:src/web/static/css --watch",
    "watch-js": "webpack --mode development --watch",
    "serve": "python src/app.py",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ffb",
    "template",
    "jasper",
    "reports",
    "agriculture",
    "palm-oil"
  ],
  "author": "FFB Template System Team",
  "license": "MIT",
  "dependencies": {
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.1",
    "@fortawesome/fontawesome-free": "^6.4.2",
    "jquery": "^3.7.1",
    "datatables.net": "^1.13.6",
    "datatables.net-bs5": "^1.13.6",
    "chart.js": "^4.4.0",
    "monaco-editor": "^0.41.0",
    "axios": "^1.5.1",
    "moment": "^2.29.4",
    "select2": "^4.1.0-rc.0",
    "sweetalert2": "^11.7.32",
    "spin.js": "^4.1.1",
    "toastr": "^2.1.4"
  },
  "devDependencies": {
    "sass": "^1.69.5",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "babel-loader": "^9.1.3",
    "@babel/core": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "css-loader": "^6.8.1",
    "style-loader": "^3.3.3",
    "file-loader": "^6.2.0",
    "url-loader": "^4.1.1",
    "clean-webpack-plugin": "^4.0.0",
    "concurrently": "^8.2.2"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}